val Byte b = 100
val Short s = 10000
val Integer i = 1000000000
val Long l = 10000000000L
val Float f = 0.5f
val Double d = 1.5

h2: Values
ul
  li b
  li s
  li i
  li l
  li f
  li d

h2: Negation
ul
  li (-b)
  li (-s)
  li (-i)
  li (-l)
  li (-f)
  li (-d)

h2: Bitwise OR
ul
  li b | b
  li s | s
  li i | i
  li l | l
  li b | l
  li i | l